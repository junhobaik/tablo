{
  "manifest_version": 2,
  "name": "Tablo - Bookmark and manage tabs",
  "description": "Bookmark and manage all the tabs in your browser.",
  "version": "1.3.2",
  "icons": {
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
